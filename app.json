{
  "expo": {
    "name": "Uber",
    "slug": "uber",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/favicon.png",
    "scheme": "uberclone",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/favicon.png",
        "backgroundColor": "#ffffff",
        "permissions": [
          "ACCESS_FINE_LOCATION",
          "ACCESS_COARSE_LOCATION",
          "ACCESS_FINE_LOCATION",
          "ACCESS_COARSE_LOCATION"
        ],
        "googleMaps": {
          "apiKey": "AIzaSyBAPdaX1Ix52kX0HlSaqULKcREFUqYdYDo"
        }
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.uber"
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://uber.com"
        }
      ],
      "expo-splash-screen",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "https://uber.com"
      },
      "eas": {
        "projectId": "14fcd37e-f4d8-4b8c-9c8d-8244b0e5c95d"
      }
    }
  }
}
